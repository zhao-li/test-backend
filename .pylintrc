[master]
load-plugins =
  pylint_django

ignore =
  .cache, ; needed for Gitlab's CI/CD
  README.md,
  app.env,
  bootstrap.sh,
  database,
  docker-compose.yml,
  docker-compose.override.yml,
  docs,
  Dockerfile,
  migrations,
  requirements.txt,
  requirements.development.txt,
  tox.ini,

[MESSAGES CONTROL]
disable =
  trailing-newlines

